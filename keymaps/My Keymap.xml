    <keyboard-shortcut first-keystroke="shift ctrl alt g" />
  </action>
  <action id="Debug">
    <keyboard-shortcut first-keystroke="alt f5" />
    <keyboard-shortcut first-keystroke="f5" />
  </action>
  <action id="Editor Copy">
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="EditorSelectWord" />
  <action id="ExpandRegion">
    <keyboard-shortcut first-keystroke="ctrl m" second-keystroke="e" />
    <keyboard-shortcut first-keystroke="ctrl m" second-keystroke="ctrl e" />
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="button2" />
    <keyboard-shortcut first-keystroke="f12" />
  </action>
  <action id="GotoImplementation">
    <keyboard-shortcut first-keystroke="ctrl alt b" />
    <keyboard-shortcut first-keystroke="shift f12" />
  </action>
  <action id="GotoNextError" />
  <action id="GotoPreviousError" />
  <action id="Graph.RouteEdges" />
